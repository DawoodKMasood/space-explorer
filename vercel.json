{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@now/node"
      },
      {
          "src": "public/**",
          "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "server.js"
      },
      {
          "src": "/(.+)",
          "dest": "public/$1"
      }
    ]
  }